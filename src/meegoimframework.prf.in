M_IM_FRAMEWORK_PREFIX=@M_IM_PREFIX@
M_IM_FRAMEWORK_INSTALL_HEADERS=@M_IM_INSTALL_HEADERS@/@MALIIT_PLUGINS_HEADER@
M_IM_PLUGINS_DIR=@M_IM_PLUGINS_DIR@
LIBS += -L@M_IM_INSTALL_LIBS@
INCLUDEPATH += $${M_IM_FRAMEWORK_INSTALL_HEADERS}
qtAddLibrary(meegoimframework)
DEFINES += @M_IM_FRAMEWORK_FEATURE@

meegoimplugin {
    target.path += $$M_IM_PLUGINS_DIR

    INSTALLS *= target
}
